{
    "name": "doctrine/guides-extension",
    "description": "Provides extra features necessary to build Doctrine docs",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Doctrine\\GuidesExtension\\": "src/"
        }
    },
    "require": {
        "symfony/dependency-injection": "^7.1",
        "phpdocumentor/guides": "^1.4"
    },
    "require-dev": {
        "doctrine/coding-standard": "^12.0",
        "phpdocumentor/guides-cli": "^1.4"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
